{
  "name": "express-serverless-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for using expressjs with serverless framework",
  "main": "lambda.js",
  "scripts": {
    "start": "export NODE_ENV='development' && nodemon ./bin/www",
    "generate-proxy": "claudia generate-serverless-express-proxy --express-module app",
    "deploy": "sls deploy"
  },
  "dependencies": {
    "aws-serverless-express": "^1.2.0",
    "body-parser": "~1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SharathHuddar/express-serverless-boilerplate.git"
  },
  "keywords": [
    "express",
    "serverless",
    "lambda",
    "AWS",
    "API",
    "gateway"
  ],
